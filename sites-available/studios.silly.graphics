server {
  listen      80;
  listen [::]:80;
  server_name studios.silly.graphics;
  return 301 https://studios.silly.graphics$request_uri;
}

server {
  listen      443 ssl http2;
  listen [::]:443 ssl http2;
  server_name studios.silly.graphics;
  root /var/www/studios.silly.graphics;
  index index.html index.htm 200.html;
  include /etc/nginx/conf.d/main.conf;
  include /etc/nginx/conf.d/directive-only/tls-paths/studios.silly.graphics;
  include /etc/nginx/conf.d/directive-only/tls.conf;
}
